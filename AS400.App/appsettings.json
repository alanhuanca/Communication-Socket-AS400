{
  "AS400ClientSettings": {
    "ServerIp": "127.0.0.1",
    "ServerPort": 9000
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/app-log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}